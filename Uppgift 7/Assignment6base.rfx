    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D BlurX;
uniform sampler2D BlurY;
uniform sampler2D NoiseMap;

uniform float Time;
uniform bool uBlendMode;
uniform sampler2D LightMap;
uniform float uNoiseFrequency;

varying vec2 texCoord;

void main(void)
{
   // Predefine white color
   vec4 white = vec4(1.0, 1.0, 1.0, 1.0); 
   
   // Offset coordinates for fun
   vec2 ntex = texCoord;
   ntex.x += Time * 50.3;
   ntex.y += Time * 70.6;
    
   // Create noise
   vec4 Noise = texture2D(NoiseMap, ntex);
   
   // Declare output color
    vec4 outCol = vec4(0, 0, 0, 0);
   
   // Copy he tex1ture coordinates
   vec2 tex1 = texCoord;
   vec2 tex2 = texCoord;
   vec2 tex3 = texCoord;
   vec2 tex4 = texCoord;
   
   // Expose coordinates to the power of eight
   tex1.x = pow(tex1.x, 6.0);
   tex1.y = pow(tex1.y, 6.0);

   // Expose coordinates
   tex2.x = pow(1.0-tex2.x, 6.0);
   tex2.y = pow(tex2.y, 6.0);

   // Expose coordinates
   tex3.x = pow(tex3.x, 6.0);
   tex3.y = pow(1.0-tex3.y, 6.0);

   // Expose coordinates
   tex4.x = pow(1.0-tex4.x, 6.0);
   tex4.y = pow(1.0-tex4.y, 6.0);
   
   // Calculate the distance between the powered tex1ture coordinates and origo
   float dist1 = distance(tex1, vec2(0, 0));
   float dist2 = distance(tex2, vec2(0, 0));
   float dist3 = distance(tex3, vec2(0, 0));
   float dist4 = distance(tex4, vec2(0, 0));
   
   // Subtract top right surfce edge through smoothstep
   // Subtract a fraction equal to the distance between the texture and origo
   dist1 = 1.0 - smoothstep(0.95, 1.0, dist1);            
   dist1 *= 1.0 - (0.3 * distance(texCoord, vec2(0, 0)));  
   
   dist2 = 1.0 - smoothstep(0.95, 1.0, dist2);
   dist2 *= 1.0 - (0.3 * distance(texCoord, vec2(0, 0)));
   
   dist3 = 1.0 - smoothstep(0.95, 1.0, dist3);
   dist3 *= 1.0 - (0.3 * distance(texCoord, vec2(0, 0)));
   
   dist4 = 1.0 - smoothstep(0.95, 1.0, dist4);
   dist4 *= 1.0 - (0.3 * distance(texCoord, vec2(0, 0)));
   
   // Color the surface based on the marked surface, fading in gray scale
   // Let boolean parameter toggle between blend modes between BlurX and BlurY
   if(uBlendMode == true){
   // Multiply blend model
      vec4 colorBlurX = texture2D(BlurX, texCoord);
      vec4 colorBlurY = texture2D(BlurY, texCoord);
      outCol += colorBlurX * colorBlurY;
   }
   
   if(uBlendMode ==  false){
   // Exclusion blend model
      vec4 colorBlurX = texture2D(BlurX, texCoord);
      vec4 colorBlurY = texture2D(BlurY, texCoord);
      outCol += colorBlurX + colorBlurY - (2.0 * colorBlurX * colorBlurY);
   } 
   
   // Mix in Noise where there is light
   outCol = mix(Noise, outCol, texture2D(LightMap, texCoord).x);
   
   // Cut the corners
   outCol *= dist1;
   outCol *= dist2;
   outCol *= dist3;
   outCol *= dist4;
   
   // Add line noise
   float lineNoise = mod(gl_FragCoord.y - 0.5, 2.0);
   lineNoise *= uNoiseFrequency;
   outCol *= 1.5 - (lineNoise * abs(sin(Time)));
   
   // Output
   gl_FragColor = outCol;
}  ]]></RmShaderCode></RmGLShader>
