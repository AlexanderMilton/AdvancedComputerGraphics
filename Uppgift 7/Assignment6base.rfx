    <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform sampler2D Intex;
uniform float Time;

varying vec2 texCoord;
uniform float Area;

float kernel[11];
vec2 offset[11];

void main(void)
{  
   // Cores indicating weight of samples, stronger closer to the center
   kernel[0]  = 0.0;
   kernel[1]  = 0.0001;
   kernel[2]  = 0.0044;
   kernel[3]  = 0.0540;
   kernel[4]  = 0.2420;
   kernel[5]  = 0.3989;
   kernel[6]  = 0.2420;
   kernel[7]  = 0.0540;
   kernel[8]  = 0.0044;
   kernel[9]  = 0.0001;
   kernel[10] = 0.0;
   
   // Sampling offset
   offset[0]  = vec2( -5, 0 );
   offset[1]  = vec2( -4, 0 );
   offset[2]  = vec2( -3, 0 );
   offset[3]  = vec2( -2, 0 );
   offset[4]  = vec2( -1, 0 );
   offset[5]  = vec2(  0, 0 );
   offset[6]  = vec2(  1, 0 );
   offset[7]  = vec2(  2, 0 );
   offset[8]  = vec2(  3, 0 );
   offset[9]  = vec2(  4, 0 );
   offset[10] = vec2(  5, 0 );
   
   // Create a texture based on the colorized texture and texture coordinates
   vec4 outCol = texture2D( Intex, texCoord );
   
   // Collect samples using offset, area coverage parameter and multibly by the kernel weight
   outCol += texture2D( Intex, texCoord + (offset[0]  * Area) ) * kernel[0];
   outCol += texture2D( Intex, texCoord + (offset[1]  * Area) ) * kernel[1];
   outCol += texture2D( Intex, texCoord + (offset[2]  * Area) ) * kernel[2];
   outCol += texture2D( Intex, texCoord + (offset[3]  * Area) ) * kernel[3];
   outCol += texture2D( Intex, texCoord + (offset[4]  * Area) ) * kernel[4];
   outCol += texture2D( Intex, texCoord + (offset[5]  * Area) ) * kernel[5];
   outCol += texture2D( Intex, texCoord + (offset[6]  * Area) ) * kernel[6];
   outCol += texture2D( Intex, texCoord + (offset[7]  * Area) ) * kernel[7];
   outCol += texture2D( Intex, texCoord + (offset[8]  * Area) ) * kernel[8];
   outCol += texture2D( Intex, texCoord + (offset[9]  * Area) ) * kernel[9];
   outCol += texture2D( Intex, texCoord + (offset[10] * Area) ) * kernel[10]; 
   
   gl_FragColor = vec4(vec3(outCol.xyz),1.0);
}]]></RmShaderCode></RmGLShader>
